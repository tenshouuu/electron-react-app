{
  "name": "electron-react-app",
  "productName": "Electron-react-app",
  "version": "0.0.1",
  "main": "main.js",
  "scripts": {
    "prod": "webpack --mode=production --config webpack.build.config.js && electron --noDevServer .",
    "build": "webpack --config webpack.build.config.js --mode=production",
    "start": "webpack-dev-server --hot --config=./webpack.dev.config.js --mode=development",
    "lint": "eslint --fix src/**/*.js src/**/*.jsx",
    "package": "npm run build",
    "postpackage": "electron-packager ./ --ignore=./src --out=./builds",
    "precommit": "lint-staged"
   },
  "author": "Peter Mikheev <pamiheevcorp@gmail.com>",
  "license": "MIT",
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.jsx": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "antd": "^3.8.4",
    "electron-devtools-installer": "^2.2.4",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.3.8",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.1.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.4",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "css-loader": "^0.28.11",
    "electron": "^2.0.3",
    "electron-builder": "^20.28.1",
    "electron-packager": "^12.1.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "mini-css-extract-plugin": "^0.4.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "postcss-nested": "^3.0.0",
    "postcss-preset-env": "^6.0.2",
    "postcss-pxtorem": "^4.0.1",
    "prettier": "^1.14.2",
    "style-loader": "^0.21.0",
    "webpack": "^4.13.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  }
}
